<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.brittany.hcd.Symptom_overview"
    android:orientation="vertical">

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="132dp"
        android:text="Choose a Day"
        android:id="@+id/textView"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#40b953"
        android:textSize="14pt"
        android:textColor="#ffffff"
        android:paddingLeft="60dp"
        android:textAlignment="center"
        android:paddingTop="50dp" />

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_row="2"
        android:layout_column="2"
        android:background="#2ca5ee"
        android:paddingTop="20dp"
        android:paddingLeft="20dp"
        android:layout_marginTop="5dp">


        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/imageButton"
            android:src="@drawable/edit_workout"
            android:layout_row="0"
            android:layout_column="0"
            android:scaleType="fitCenter"
            android:background="@null"
            android:paddingBottom="10dp"
            android:onClick="Edit_clicked"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="15 oct 2015"
            android:id="@+id/textView2"
            android:textSize="12pt"
            android:textColor="#ffffff"
            android:layout_row="0"
            android:layout_column="1"
            android:paddingLeft="40dp" />

        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/imageButton12"
            android:src="@drawable/delete_workout"
            android:layout_row="0"
            android:layout_column="5"
            android:scaleType="fitCenter"
            android:background="@null"
            android:paddingBottom="10dp"
            android:layout_marginRight="40dp"
            android:onClick="delete_clicked"/>
    </GridLayout>

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_row="2"
        android:layout_column="2"
        android:background="#2ca5ee"
        android:paddingTop="20dp"
        android:paddingLeft="20dp"
        android:layout_marginTop="5dp">


        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@null"
            android:id="@+id/imageButton2"
            android:layout_row="0"
            android:layout_column="0"
            android:src="@drawable/edit_workout"
            android:scaleType="fitCenter"
            android:paddingBottom="10dp"
            android:onClick="Edit_clicked"/>

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="14 oct 2015"
            android:id="@+id/textView3"
            android:textSize="12pt"
            android:textColor="#ffffff"
            android:layout_row="0"
            android:layout_column="1"
            android:paddingLeft="40dp" />

        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/imageButton13"
            android:src="@drawable/delete_workout"
            android:layout_row="0"
            android:layout_column="7"
            android:scaleType="fitCenter"
            android:background="@null"
            android:paddingBottom="10dp"
            android:layout_marginRight="40dp"
            android:onClick="delete_clicked"/>
    </GridLayout>
    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_row="2"
        android:layout_column="2"
        android:background="#2ca5ee"
        android:paddingTop="20dp"
        android:paddingLeft="20dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp">


        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/imageButton3"
            android:layout_row="0"
            android:src="@drawable/edit_workout"
            android:layout_column="0"
            android:onClick="Edit_clicked"
            android:background="@null"
            android:scaleType="fitCenter"
            android:paddingBottom="10dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=" 15 oct 2015"
            android:id="@+id/textView4"
            android:textSize="12pt"
            android:textColor="#ffffff"
            android:layout_row="0"
            android:layout_column="1"
            android:paddingLeft="40dp" />
        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/imageButton14"
            android:src="@drawable/delete_workout"
            android:layout_row="0"
            android:layout_column="7"
            android:scaleType="fitCenter"
            android:background="@null"
            android:paddingBottom="10dp"
            android:layout_marginRight="40dp"
            android:onClick="delete_clicked"/>
    </GridLayout>

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_row="2"
        android:layout_column="2"
        android:background="#2ca5ee"
        android:paddingTop="20dp"
        android:paddingLeft="20dp">

        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/imageButton17"
            android:src="@drawable/edit_workout"
            android:onClick="Edit_clicked"
            android:layout_row="0"
            android:layout_column="0"
            android:scaleType="fitCenter"
            android:background="@null"
            android:paddingBottom="10dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="16 oct 2015"
            android:id="@+id/Date_textview"
            android:textSize="12pt"
            android:textColor="#ffffff"
            android:layout_row="0"
            android:layout_column="1"
            android:paddingLeft="40dp" />

        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/imageButton16"
            android:src="@drawable/delete_workout"
            android:layout_row="0"
            android:layout_column="5"
            android:scaleType="fitCenter"
            android:background="@null"
            android:paddingBottom="10dp"
            android:layout_marginRight="40dp"
            android:onClick="delete_clicked"/>
    </GridLayout>
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text=""
        android:id="@+id/blank"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#40b953"
        android:textSize="12pt"
        android:textColor="#ffffff"
        android:paddingLeft="0dp"
        android:layout_weight="0.53"
        android:layout_marginTop="5dp" />
</LinearLayout>
